@model IEnumerable<Dominio.Classes.HistoricoAcesso>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Criar Novo", "Cadastrar")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.idHistoricoAcesso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.idUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.idPacoteHospedagem)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.observacao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dataCadastro)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idHistoricoAcesso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idPacoteHospedagem)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.observacao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dataCadastro)
        </td>
        <td>
            @Html.ActionLink("Editar", "Atualizar", new { id=item.idHistoricoAcesso }) |
            @Html.ActionLink("Detalhar", "Detalhar", new { id = item.idHistoricoAcesso }) |
            @Html.ActionLink("Excluir", "Excluir", new { id = item.idHistoricoAcesso })
        </td>
    </tr>
}

</table>
